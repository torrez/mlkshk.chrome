{
	"name": "mlkshk",
	"version": "1.2.3",
	"manifest_version": 2,
	"description": "Post photos to mlkshk.com.",
	"browser_action": {
	    "default_icon": "icon.png",
		"default_popup": "menu.html",
        "default_title": "MLKSHK" 
	},
	"icons": {
        "16": "icon_16.png",
        "48": "icon.png"
	},
    "background": {
        "page": "background.html"
    },
	"permissions": [ "tabs", "contextMenus", "http://*/*", "http://*/", "https://*/" ]
}
